{
    "version": 2,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.9.0"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "public/build/$1"
        },
        {
            "src": "/(css|js|images)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/favicon.ico",
            "dest": "public/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_KEY": "base64:abcdefghijklmnopqrstuvwxyz1234567890ABCDEFGHIJ",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "array",
        "LOG_CHANNEL": "stderr"
    }
}
